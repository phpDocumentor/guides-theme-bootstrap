<div class="level-x">
    {% for entry in node.menuEntries -%}
        {% apply spaceless %}
        <a href="{{ renderLink(entry.url) }}"
           class="nav-link {% if entry.options.active %} active {% endif %}"
                {% if entry.options.active %}  aria-current="page" {% endif %} >
            {{ renderNode(entry.value.value) }}
        </a>
        {% endapply %}
    {% endfor %}
</div>
